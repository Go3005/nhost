{
  "name": "@nhost/functions-helpers",
  "version": "0.0.1",
  "description": "Nhost Functions Helpers",
  "license": "MIT",
  "keywords": [
    "graphql",
    "api",
    "nhost",
    "hasura",
    "serverless",
    "functions",
    "lambda"
  ],
  "author": "Nhost",
  "homepage": "https://nhost.io",
  "bugs": "https://github.com/nhost/nhost/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/nhost/nhost.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "run-p dev:lib \"bundle-functions --watch\"",
    "dev:lib": "vite build --config ../../config/vite.lib.dev.config.js",
    "build": "vite build",
    "bundle-functions": "node functions-bundler.js tests functions",
    "e2e": "start-test e2e:backend http-get://localhost:9695 e2e:frontend",
    "e2e:backend": "nhost dev --no-browser",
    "e2e:frontend": "run-s bundle-functions e2e:test",
    "e2e:test": "vitest run",
    "prettier": "prettier --check src/",
    "prettier:fix": "prettier --write src/",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "verify": "run-p prettier lint",
    "verify:fix": "run-p prettier:fix lint:fix"
  },
  "dependencies": {
    "@nhost/nhost-js": "workspace:*",
    "axios": "^0.27.2",
    "date-fns": "^2.29.3",
    "graphql": "16.6.0",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.9",
    "command-line-args": "^5.2.1",
    "dotenv": "^16.0.3",
    "fs-extra": "^10.1.0",
    "typescript": "^4.8.4"
  }
}